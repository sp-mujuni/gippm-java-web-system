    </servlet>
    <servlet>
        <servlet-name>DeliveryNotification</servlet-name>
        <servlet-class>foodshelf.DeliveryNotification</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Add_stockSize</servlet-name>
        <url-pattern>/Add_stockSize</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Add_staff</servlet-name>
        <url-pattern>/Add_staff</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Cart</servlet-name>
        <url-pattern>/Cart</url-pattern>
    </servlet-mapping>
