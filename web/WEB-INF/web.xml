    <servlet>
        <servlet-name>Add_stockSize</servlet-name>
        <servlet-class>foodshelf.Add_stockSize</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Add_staff</servlet-name>
        <servlet-class>foodshelf.Add_staff</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Cart</servlet-name>
        <servlet-class>foodshelf.Cart</servlet-class>
    </servlet>
